<keymap version="1" name="One-Handed-Keymap" parent="$default">
  <action id="$Redo">
    <keyboard-shortcut first-keystroke="shift ctrl z" />
    <keyboard-shortcut first-keystroke="shift alt back_space" />
    <keyboard-shortcut first-keystroke="ctrl y" />
  </action>
  <action id="ChooseDebugConfiguration">
    <keyboard-shortcut first-keystroke="shift alt f9" />
    <keyboard-shortcut first-keystroke="shift ctrl alt d" />
  </action>
  <action id="ChooseRunConfiguration">
    <keyboard-shortcut first-keystroke="shift alt f10" />
    <keyboard-shortcut first-keystroke="shift ctrl alt r" />
  </action>
  <action id="CommentByBlockComment">
    <keyboard-shortcut first-keystroke="shift ctrl slash" />
    <keyboard-shortcut first-keystroke="shift ctrl divide" />
    <keyboard-shortcut first-keystroke="shift alt c" />
  </action>
  <action id="CommentByLineComment">
    <keyboard-shortcut first-keystroke="ctrl slash" />
    <keyboard-shortcut first-keystroke="ctrl divide" />
    <keyboard-shortcut first-keystroke="alt c" />
  </action>
  <action id="Console.TableResult.CloneColumn" />
  <action id="Console.TableResult.DeleteRows" />
  <action id="Console.TableResult.EditFilterCriteria" />
  <action id="Editor Redo">
    <keyboard-shortcut first-keystroke="ctrl y" />
  </action>
  <action id="EditorDeleteLine">
    <keyboard-shortcut first-keystroke="alt d" />
  </action>
  <action id="FileStructurePopup">
    <keyboard-shortcut first-keystroke="ctrl f12" />
    <keyboard-shortcut first-keystroke="shift ctrl alt f" />
  </action>
  <action id="IntroduceActionsGroup">
    <keyboard-shortcut first-keystroke="shift alt t" />
  </action>
  <action id="RecentChanges" />
  <action id="RenameElement">
    <keyboard-shortcut first-keystroke="shift f6" />
    <keyboard-shortcut first-keystroke="alt t" />
  </action>
  <action id="RunClass">
    <keyboard-shortcut first-keystroke="shift ctrl f10" />
    <keyboard-shortcut first-keystroke="ctrl alt r" />
  </action>
  <action id="ShowUsages">
    <keyboard-shortcut first-keystroke="ctrl alt f7" />
    <keyboard-shortcut first-keystroke="alt f" />
  </action>
  <action id="ToggleBookmark" />
  <action id="ToggleFullScreen">
    <keyboard-shortcut first-keystroke="f11" />
  </action>
  <action id="UML.ShowChanges" />
  <action id="tasks.switch" />
</keymap>